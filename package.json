{
  "name": "dotto-ai",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/server.ts",
    "dev:viewer": "npm run dev --prefix src/viewer",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:viewer\" --names \"backend,viewer\" --prefix-colors \"cyan,magenta\"",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "run": "node dist/server.js --once"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@natalietdg/dotto": "^1.1.3",
    "dotenv": "^17.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "concurrently": "^9.1.0",
    "tsx": "^4.7.0",
    "typescript": "^5.6.0"
  }
}
