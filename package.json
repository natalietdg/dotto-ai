{
  "name": "dotto-ai",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/server.ts",
    "dev:viewer": "npm run dev --prefix src/viewer",
    "dev:all": "concurrently \"npm run dev\" \"sleep 2 && npm run dev:viewer\" --names \"backend,viewer\" --prefix-colors \"cyan,magenta\"",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "run": "node dist/server.js --once",
    "verify": "tsx src/cli/dotto-verify.ts",
    "enforce": "tsx src/server.ts --enforce",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,json,css,md}\""
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@hashgraph/sdk": "^2.51.0",
    "dotenv": "^17.2.3",
    "fast-glob": "^3.3.2",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "concurrently": "^9.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.6.0"
  }
}
